{
  "name": "group-chat",
  "version": "1.0.0",
  "description": "Build a simple application which provides web services to facilitate group chat",
  "main": "app.js",
  "scripts": {
    "start": "npm-run-all p:server",
    "build": "pnpm install && npm-run-all clean transpile",
    "build:dev": "export NODE_ENV=development && pnpm build",
    "p:server": "node ./dist/bin/server.js",
    "dev": "export NODE_ENV=development && npm-run-all build p:server",
    "transpile": "babel ./src --copy-files --out-dir dist",
    "rimraf": "./node_modules/rimraf/bin.js",
    "clean": "rimraf dist"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "author": "sai pavan kumar",
  "license": "ISC",
  "dependencies": {
    "@babel/node": "^7.25.0",
    "babel-loader": "^9.1.3",
    "bson": "^6.8.0",
    "cls-hooked": "^4.2.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonpath": "^1.1.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.5.4",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.4",
    "ramda": "^0.30.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^10.0.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "swagger-autogen": "^2.23.7",
    "webpack-dev-server": "^5.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf":"~6.0.1"
  }
}
